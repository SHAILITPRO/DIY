<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parallel Test Suite" parallel="tests">


    <test name="ChromeTest">
        <parameter name="browserName" value="chrome"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="diyLambda.LoginLT" />
        </classes>
    </test>
    
    
    <test name="FirefoxTest">
        <parameter name="browserName" value="firefox"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 11"/>
        <classes>
            <class name="diyLambda.LoginLT" />
        </classes>
    </test>
    
    <test name="SafariTest">
        <parameter name="browserName" value="Safari"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="MacOS Catalina"/>
        <classes>
            <class name="diyLambda.LoginLT" />
        </classes>
    </test>
    
    <test name="edgeTest">
        <parameter name="browserName" value="Edge"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 8"/>
        <classes>
            <class name="diyLambda.LoginLT" />
        </classes>
    </test>
    
</suite>
